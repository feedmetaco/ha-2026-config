        "id": 1,
        "title": "Total Entities",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 4,
          "x": 0,
          "y": 0
        },
        "options": {
          "graphMode": "none",
          "textMode": "value_and_name",
          "colorMode": "value"
        },
        "targets": [
          {
            "rawSql": "SELECT COUNT(DISTINCT entity_id) FROM states",
            "format": "table",
            "datasource": "HomeAssistant_InfluxDB"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 2000
                },
                {
                  "color": "red",
                  "value": 3000
                }
              ]
            }
          }
